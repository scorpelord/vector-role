api:
  enabled: true
  address: "127.0.0.1:8686"

sources:
  generated_logs:
    type: demo_logs
    format: apache_common
    interval: 1

sinks:
  to_clickhouse:
    type: clickhouse
    inputs:
      - generated_logs
    endpoint: "http://localhost:8123"
    database: default
    table: logs
    skip_unknown_fields: true
